/**
 * _strlen - gets the length of a string
 * @str: pointer to the sring whose length is to be determined
 *
 * Return: length of the string
 */

int _strlen(char *str)
{
	int len = 0;

	while (*str)
	{
		++len;
		++str;
	}
	return (len);
}
